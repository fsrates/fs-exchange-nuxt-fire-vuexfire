{
  "name": "nuxt-fire-vuexfire-example",
  "version": "1.0.0",
  "description": "Example project showcasing the use of @nuxtjs/firebase together with vuexfire.",
  "author": "Pascal Luther",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "dev": "nuxt",
